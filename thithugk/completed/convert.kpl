PROGRAM CONVERT;
VAR N : INTEGER;

PROCEDURE INPUT;
BEGIN
    N := READI;
END;

PROCEDURE CONV;
BEGIN
    IF N >= 8 THEN CALL WRITEC('A')
    ELSE IF N >= 7 THEN CALL WRITEC('B')
    ELSE IF N >= 6 THEN CALL WRITEC('C')
    ELSE IF N >= 4 THEN CALL WRITEC('D')
    ELSE CALL WRITEC('F');
END;

BEGIN
    CALL INPUT;
    CALL CONV;
END.